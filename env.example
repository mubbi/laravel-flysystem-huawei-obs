# Huawei OBS Configuration
HUAWEI_OBS_ACCESS_KEY_ID=your_access_key_id_here
HUAWEI_OBS_SECRET_ACCESS_KEY=your_secret_access_key_here
HUAWEI_OBS_BUCKET=your_bucket_name_here
HUAWEI_OBS_ENDPOINT=https://obs.cn-north-1.myhuaweicloud.com
HUAWEI_OBS_REGION=cn-north-1
HUAWEI_OBS_PREFIX=
HUAWEI_OBS_SECURITY_TOKEN=your_security_token_here

# Huawei OBS HTTP Client Configuration
HUAWEI_OBS_TIMEOUT=120
HUAWEI_OBS_CONNECT_TIMEOUT=30
HUAWEI_OBS_VERIFY_SSL=true

# Huawei OBS Retry Configuration
HUAWEI_OBS_RETRY_ATTEMPTS=3
HUAWEI_OBS_RETRY_DELAY=1

# Huawei OBS Logging Configuration
HUAWEI_OBS_LOGGING_ENABLED=false
HUAWEI_OBS_LOG_OPERATIONS=false
HUAWEI_OBS_LOG_ERRORS=true

# Application Configuration
APP_NAME="Laravel Flysystem Huawei OBS"
# 
# @author  Mubbasher Ahmed <hello@mubbi.me>
# @link    https://mubbi.me
# @license MIT
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Database Configuration (if needed for testing)
DB_CONNECTION=sqlite
DB_DATABASE=database/database.sqlite

# Cache and Session
CACHE_DRIVER=array
SESSION_DRIVER=array
QUEUE_DRIVER=sync 